     <div id="panel-2" class="panel panel-default">
                         <div class="panel-heading">
                <span class="title elipsis pull-left">
                        <strong>Liste des gagnants</strong> 
                        </span>                         
                  <!-- tabs nav -->
                  <ul class="nav nav-tabs pull-right">
                    <li class="active"><!-- TAB 1 -->
                      <a href="#ice_red" data-toggle="tab" aria-expanded="true">Free issues</a>
                    </li>
                    <li class=""><!-- TAB 2 -->
                      <a href="#ice_black" data-toggle="tab" aria-expanded="true">Rewards item</a>
                    </li>                                                                                                     
                  </ul>
                  <!-- /right options -->
                   </div>
                                <!-- panel content -->
                    <div class="panel-body">  
                      <!-- tabs content -->
                    <div class="tab-content transparent"> 
                      <div id="ice_red" class="tab-pane active"><!-- TAB 1 CONTENT -->                               
                                           <table class="table table-striped table-bordered table-hover" id="fi">
                                                    <thead>
                                                        <tr>
                                                            <th class="col-md-2">Nom</th>
                                                             <th>Age</th>
                                                            <th>Telephone</th>
                                                             
                                                            <th>Objet</th>
                                                            <th>Date</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                    {% for gagnant in gagnants if gagnant.object=='Bouteille' %}
                                                        <tr class="odd gradeX">  
                                                            <td >{{gagnant.nom}}</td>  
                                                            <td >{{gagnant.age}}</td>                                 
                                                            <td >{{gagnant.tel}}</td>  
                                                            <td >{{gagnant.object}}</td>
                                                            <td >{{gagnant.date|date('d/m/Y')}}</td> 

                                                        {%endfor%}
                                                    </tbody>
                                                </table>

                                    </div><!-- /TAB 1 CONTENT--> 

                      <div id="ice_black" class="tab-pane"><!-- TAB 1 CONTENT -->                               
                                           <table class="table table-striped table-bordered table-hover" id="ri">
                                                    <thead>
                                                        <tr>
                                                            <th class="col-md-2">Nom</th>
                                                             <th>Age</th>
                                                            <th>Telephone</th>
                                                             
                                                            <th>Objet</th>
                                                            <th >Date</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                    {% for gagnant in gagnants if gagnant.object!='Bouteille'%}
                                                        <tr class="odd gradeX">  
                                                            <td >{{gagnant.nom}}</td> 
                                                            <td >{{gagnant.age}}</td>                                   
                                                            <td >{{gagnant.tel}}</td>  
                                                            <td >{{gagnant.object}}</td>
                                                            <td >{{gagnant.date|date('d/m/Y')}}</td> 
                                                        {%endfor%}
                                                    </tbody>
                                                </table>

                                    </div><!-- /TAB 1 CONTENT-->                                                                                                                                   
                        </div><!-- /TAB 1 CONTENT --> 
                                <a class="btn btn-xs btn-info"  href="#">
                                                    <i class="fa fa-arrow-right"></i> 
                                                    Exporter vers (.xls)
                                    </a>
                                </div>
                                <!-- /panel content -->
                    </div>
